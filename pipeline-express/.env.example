# Supabase Configuration (for saving workflows)
REACT_APP_SUPABASE_URL=your-supabase-url
REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# Azure DevOps Configuration (for dynamic module loading)
# Leave these blank to use default hardcoded modules

# Your Azure DevOps organization name (e.g., 'myorg' from https://dev.azure.com/myorg)
REACT_APP_ADO_ORG=

# Project name in Azure DevOps
REACT_APP_ADO_PROJECT=

# Repository name where module definitions are stored
REACT_APP_ADO_REPO=

# Personal Access Token (PAT) with 'Code (Read)' permission
# Generate at: https://dev.azure.com/{org}/_usersSettings/tokens
REACT_APP_ADO_PAT=

# Path to the folder containing module YAML files (default: 'modules')
REACT_APP_ADO_MODULES_PATH=modules

# Branch to fetch modules from (default: 'main')
REACT_APP_ADO_BRANCH=main
