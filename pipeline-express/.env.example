# Supabase Configuration (for saving workflows)
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_SUPABASE_URL=your-supabase-url
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_SUPABASE_ANON_KEY=your-supabase-anon-key

# Azure DevOps Configuration (for dynamic module loading)
# Leave these blank to use default hardcoded modules

# Your Azure DevOps organization name (e.g., 'myorg' from https://dev.azure.com/myorg)
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_ADO_ORG=

# Project name in Azure DevOps
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_ADO_PROJECT=

# Repository name where module definitions are stored
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_ADO_REPO=

# Personal Access Token (PAT) with 'Code (Read)' permission
# Generate at: https://dev.azure.com/{org}/_usersSettings/tokens
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_ADO_PAT=

# Path to the folder containing module YAML files (default: 'modules')
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_ADO_MODULES_PATH=modules

# Branch to fetch modules from (default: 'main')
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_ADO_BRANCH=main

# n8n Webhook Configuration
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_N8N_WEBHOOK_URL=http://localhost:5678/webhook

# Keycloak Configuration
HYPERPLANE_CUSTOM_SECRET_KEY_KEYCLOAK_URL=http://localhost:8080
HYPERPLANE_CUSTOM_SECRET_KEY_KEYCLOAK_REALM=myrealm
HYPERPLANE_CUSTOM_SECRET_KEY_KEYCLOAK_CLIENT_ID=pipeline-express
HYPERPLANE_CUSTOM_SECRET_KEY_KEYCLOAK_CLIENT_SECRET=

# Proxy Server Configuration
HYPERPLANE_CUSTOM_SECRET_KEY_PROXY_PORT=3001
HYPERPLANE_CUSTOM_SECRET_KEY_PROXY_ALLOWED_ORIGINS=http://localhost:3000,https://localhost:3000

# Enable proxy mode (routes API calls through authentication proxy)
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_USE_PROXY=true
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_PROXY_URL=http://localhost:3001

# Service Account for Backend API Calls (NUID credentials)
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_SERVICE_ACCOUNT_NUID=your-service-account-nuid
HYPERPLANE_CUSTOM_SECRET_KEY_REACT_APP_SERVICE_ACCOUNT_PASSWORD=your-service-account-password
